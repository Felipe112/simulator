<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
        "https://checkstyle.org/dtd/suppressions_1_2.dtd">

<suppressions>
    <suppress files="[/\\]test[/\\].*" checks="Javadoc.*" />
    <suppress files="[/\\]target[/\\].*" checks=".*" />
    <suppress files="[/\\]target-eclipse[/\\].*" checks=".*" />
    <suppress files=".*\.end2end\.log.*" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*\.gif" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*\.png" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*\.jpg" checks="[a-zA-Z0-9]*"/>
    <suppress files=".*\.csv" checks="[a-zA-Z0-9]*"/>
</suppressions>
